
<script>
  $(document).ready(function(){
    JustWrite.initialize(); 
    setEditableElements();
    autoSavePageContent(); 
  });


  window.unload = function() {
    saveCurrentPageDimensions({async:false});

    _.each($('.text'), function(div) {
      var text = div.text();
      var pageID = div.parentsUntil('.page').id;
      saveHTMLText(text, pageID);
    });
  };

</script>


